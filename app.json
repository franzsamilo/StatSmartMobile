{
  "expo": {
    "name": "StatSmart",
    "slug": "statsmart-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_copy.png",
    "scheme": "statsmartmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "backendUrl": "http://localhost:3000",
      "router": {},
      "eas": {
        "projectId": "1c9b7af2-bdc4-41de-b5ff-637ca574f7f7"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.statsmart.app",
      "versionCode": 1,
      "icon": "./assets/images/icon_copy.png",
      "adaptiveIcon": {
        "backgroundColor": "#061428",
        "foregroundImage": "./assets/images/icon_copy.png",
        "backgroundImage": "",
        "monochromeImage": ""
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_copy.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#061428",
          "dark": {
            "backgroundColor": "#061428"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
